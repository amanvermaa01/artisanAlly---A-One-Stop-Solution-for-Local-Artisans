{
  "version": 2,
  "builds": [
    {
      "src": "artisanAlly-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "artisanAlly-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/artisanAlly-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/artisanAlly-frontend/$1"
    }
  ]
}
